{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "rules": {
    "import/no-unresolved": "off",
    "no-console": "off"
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
